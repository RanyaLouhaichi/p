    // Animate metrics
    function initializeMetrics() {
        var metrics = document.querySelectorAll('.metric-value');
        for (var i = 0; i < metrics.length; i++) {
            var metric = metrics[i];
            var value = parseFloat(metric.getAttribute('data-value'));
            var suffix = metric.getAttribute('data-suffix') || '';
            animateValue(metric, 0, value, 1500, suffix);
        }
    }
    
    function animateValue(element, start, end, duration, suffix) {
        var startTime = performance.now();
        var isFloat = !Number.isInteger(end);
        
        function update(currentTime) {
            var elapsed = currentTime - startTime;
            var progress = Math.min(elapsed / duration, 1);
            
            var easeOut = 1 - Math.pow(1 - progress, 4);
            var current = start + (end - start) * easeOut;
            
            if (isFloat) {
                element.textContent = current.toFixed(1) + suffix;
            } else {
                element.textContent = Math.floor(current) + suffix;
            }
            
            if (progress < 1) {
                requestAnimationFrame(update);
            }
        }
        
        requestAnimationFrame(update);
    }
    
    // Initialize all charts
    function initializeCharts() {
        // Chart defaults
        Chart.defaults.font.family = '-apple-system, BlinkMacSystemFont, "Segoe UI", sans-serif';
        Chart.defaults.color = '#172B4D';
        
        // Sprint Progress Chart (Stacked Bar)
        new Chart(document.getElementById('sprintProgressChart'), {
            type: 'bar',
            data: {
                labels: ['Week 1', 'Week 2', 'Week 3', 'Week 4'],
                datasets: [{
                    label: 'Completed',
                    data: [15, 28, 42, 47],
                    backgroundColor: '#0052CC'
                }, {
                    label: 'In Progress',
                    data: [8, 12, 8, 6],
                    backgroundColor: '#4C9AFF'
                }, {
                    label: 'To Do',
                    data: [27, 10, 0, 0],
                    backgroundColor: '#DEEBFF'
                }]
            },
            options: {
                responsive: true,
                maintainAspectRatio: false,
                scales: {
                    x: { stacked: true },
                    y: { stacked: true }
                }
            }
        });
        
        // Velocity Trend Chart
        new Chart(document.getElementById('velocityTrendChart'), {
            type: 'line',
            data: {
                labels: ['Sprint 1', 'Sprint 2', 'Sprint 3', 'Sprint 4', 'Sprint 5', 'Sprint 6'],
                datasets: [{
                    label: 'Actual',
                    data: [32, 35, 38, 42, 45, 47],
                    borderColor: '#0052CC',
                    backgroundColor: 'rgba(0, 82, 204, 0.1)',
                    tension: 0.3
                }, {
                    label: 'Planned',
                    data: [30, 35, 40, 40, 45, 50],
                    borderColor: '#6B778C',
                    borderDash: [5, 5],
                    tension: 0.3
                }]
            },
            options: {
                responsive: true,
                maintainAspectRatio: false
            }
        });
        
        // Issue Distribution Pie Chart
        new Chart(document.getElementById('issueDistributionChart'), {
            type: 'doughnut',
            data: {
                labels: ['Bugs', 'Features', 'Tasks', 'Tech Debt'],
                datasets: [{
                    data: [15, 45, 30, 10],
                    backgroundColor: ['#DE350B', '#4C8BF5', '#AFA8D7', '#FCD169']

                }]
            },
            options: {
                responsive: true,
                maintainAspectRatio: false,
                plugins: {
                    legend: {
                        position: 'bottom'
                    }
                }
            }
        });
        
        // Team Workload Chart
        new Chart(document.getElementById('teamWorkloadChart'), {
            type: 'bar',
