            emilyLoad: 90
        };
        
        for (var id in loads) {
            if (loads.hasOwnProperty(id)) {
                var value = loads[id];
                var canvas = document.getElementById(id);
                if (canvas) {
                    new Chart(canvas, {
                        type: 'doughnut',
                        data: {
                            datasets: [{
                                data: [value, 100 - value],
                                backgroundColor: [
                                    value > 100 ? '#F87171' : value > 80 ? '#FCD34D' : '#7DD3C0',
                                    '#F4F5F7'
                                ],
